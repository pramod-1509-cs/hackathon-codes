// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;
int main() {
 int cnt=0;
 string s;
 cin>>s;
 if(s=="()")
 cout<<" ";
 else{
     for(int i=0;i<s.length()-1;i++){
     if(s[i]=='(' && s[i+1]==')')
     cnt+=1;
 }
 for(int i=0;i<cnt;i++)
 cout<<"()";
 }
 
}
